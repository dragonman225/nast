{
  "name": "nast-util-to-html",
  "version": "0.1.0",
  "description": "Transform NAST to HTML.",
  "main": "dist/nast-util-to-html/src/index.js",
  "scripts": {
    "build:dev_webpack": "webpack --mode=development && npm run report:deps_graph",
    "build:dev_tsc": "tsc && npm run report:deps_graph",
    "build:prod": "webpack --mode=production && npm run report:deps_graph",
    "test": "node test/index.js",
    "report:deps_graph": "npx depcruise --exclude '^node_modules' --output-type dot --prefix 'https://github.com/dragonman225/nast-util-to-html/tree/master/' src/index.ts | dot -T svg > report/deps_graph.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dragonman225/nast-util-to-html.git"
  },
  "keywords": [
    "notion",
    "nast",
    "util",
    "notajs"
  ],
  "author": "dragonman225",
  "license": "MIT",
  "dependencies": {
    "katex": "^0.10.2",
    "prismjs": "^1.16.0"
  },
  "devDependencies": {
    "@types/katex": "^0.10.1",
    "@types/node": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "dependency-cruiser": "^5.0.0",
    "eslint": "^5.16.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.1",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6"
  },
  "bugs": {
    "url": "https://github.com/dragonman225/nast-util-to-html/issues"
  },
  "homepage": "https://github.com/dragonman225/nast-util-to-html"
}
